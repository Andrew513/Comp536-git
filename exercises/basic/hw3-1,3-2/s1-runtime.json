{
  "target": "bmv2",
  "p4info": "build/basic.p4.p4info.txtpb",
  "bmv2_json": "build/basic.json",
  "table_entries": [
    {
      "table": "MyIngress.ecmp_group",
      "match": { "hdr.ipv4.dstAddr": ["10.0.2.0", 24] },
      "action_name": "MyIngress.set_flowlet_select",
      "action_params": { "ecmp_base": 0, "ecmp_count": 2, "timeout_us": 25000  }
    },
    {
      "table": "MyIngress.ecmp_nhop",
      "match": { "meta.ecmp_select": 0 },
      "action_name": "MyIngress.set_nhop",
      "action_params": { "nhop_dmac": "00:00:00:01:02:00", "port": 2 }
    },
    {
      "table": "MyIngress.ecmp_nhop",
      "match": { "meta.ecmp_select": 1 },
      "action_name": "MyIngress.set_nhop",
      "action_params": { "nhop_dmac": "00:00:00:01:03:00", "port": 3 }
    },
    {
      "table": "MyIngress.ipv4_lpm",
      "match": { "hdr.ipv4.dstAddr": ["10.0.1.1", 32] },
      "action_name": "MyIngress.ipv4_forward",
      "action_params": { "dmac": "08:00:00:00:01:11", "port": 1 }
    }
  ]
}
